function title:update()
	if (ctrl==0 and (btn(4) or btn(5))) then
		ctrl=fs
		changestate(2)
	end
end

function title:draw()
	cls()
	local ox=16
	local oy=8
	local title={16,17,18,18,19,20,21,22,23,20,64,64,24,22,25,26,20,24,27,0,28,29,0,30,31,20,80,19,28,27,30,0,16,28,30,22,30,28}
	for s in all(title) do
		ox+=8
		oy+=flr(8*flr(s/64))
		if (s>=64) then ox=abs(64-s) end
		spr(s,ox,oy)
	end
	spr(8+flr(dt/16),16,32)
	spr(8+flr(dt/16),103,7,1,1,true)
