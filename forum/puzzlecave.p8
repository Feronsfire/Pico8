		print("press x button to play",18,98,12)
	end
	print("v1.0 2015 - www.hackefuffel.com",4,118,1)
end

function intro:update()
	if (ctrl==0 and (btn(4) or btn(5))) then
		ctrl=fs
		changestate(3)
	end
end

function intro:draw()
