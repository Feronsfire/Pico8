--init function
function game_ini()
 --define player
 p = {}
 p.x = 22
 p.y = 216
 p.hor = 0
 p.ver = 0
 p.dir = true
 p.bottom = p.y+8
 p.idle = false
 p.animframe = 0
 
 level = 1
 sglvl = 0
 lgain = 1
 death = 0
 
 --scrolling stuff
 scroll = 0
 scrollgain = 0.5
 scrolltimer = 11
 
 gstate = 0
 
 camera(0,128)
 fadepal(7,7,7)
 _draw()
 fadein()
end

function pdraw(i,a,b)
 spr(a,p.x-8,p.y-8+i,2,1,p.dir)
 spr(b,p.x-8,p.y+i,2,1,p.dir)
end

function pdie()
 death = 1
 sfx(2)
 endtimer = 50
 pghosty = p.y-16
 pghostdir = false
end

--collision detection
function solid(a,b)
 a /= 8
 b /= 8
 sld = mget(a,b)
 return fget(sld,0)
end

function tcolhor(a,b)
 if solid(a,b-8)
 or solid(a,b)
 or solid(a,b+7) then
  return true end
end

function tcolver(a,b)
 if solid(a-8,b)
 or solid(a,b)
 or solid(a+7,b) then
  return true end
end

function copymap(o,a,b)
 for i=0,15 do
  for j=0,15 do
   mset(i,o+j,mget(a+i,b+j))
  end
 end
end

function mapupdate()
 if scroll >= 128 then
  copymap(16,0,0)
  p.y += 128
  local rx = flr(rnd(2))*16
  local ry = flr(rnd(2))*16
  copymap(0,rx+32,ry)
  scroll = 0
  _draw()
  lgain = 1
 end
 
 if scrolltimer > 0 then
  scrolltimer -= 0.1
 else
  scroll += scrollgain
 end
end

--fade effects
function fadepal(a,b,c)
 pal(1,a,1)
 pal(13,b,1)
 pal(6,c,1)
 flip()
 flip()
 flip()
end

function fadeout()
 fadepal(13,6,7)
 fadepal(6,7,7)
 fadepal(7,7,7)
end

function fadein()
 fadepal(6,7,7)
 fadepal(13,6,7)
 pal()
end

