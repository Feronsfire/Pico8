--player logic
function plogic()
 --round temp values
 thor = 0
 tver = 0
 --gravity
 if tcolver(p.x,p.bottom) then
  p.ver = 0
 else
  p.ver += 0.5
 end
 tver = flr(p.ver)
 
 --downward collision
 if p.ver > 0 then
  if tcolver(p.x,p.bottom+tver) then
   local gdist = p.bottom+1
   for i=1,tver do
    if not tcolver(p.x,p.bottom+i) then
     gdist += 1
    end
   end
   while (p.bottom < gdist) do
    p.y += 1
    p.bottom = p.y+8
   end
   p.ver = 0
   sfx(1)
  end
 end
 tver = flr(p.ver)
 
 --upwards collision
 if p.ver < 0 then
  if tcolver(p.x,p.y-9+tver) then
   local cdist = p.y-10
   for i=1,abs(tver) do
    if tcolver(p.x,p.y-9-i) then
     cdist -= 1
    end
   end
   while (p.y-9 > cdist) do
    p.y -= 1
   end
   p.ver = 0
  end
 end
 
 --horizontal movement
 --left
 if btn(0) then
  if tcolhor(p.x-9,p.y) then
   p.hor = 0
  else
   p.hor -= 1
  end
  p.dir = false
 end
 --right
 if btn(1) then
  if tcolhor(p.x+8,p.y) then
   p.hor = 0
  else
   p.hor += 1
  end
  p.dir = true
 end
 
 thor = flr(p.hor)
 abshor = abs(thor)
 
 if p.hor < 0 then
  if tcolhor(p.x-8+thor,p.y) then
   local wdist = p.x-8
   for i=1,abshor do
    if not tcolhor(p.x-8-i,p.y) then
     wdist -= 1
    end
   end
   while (p.x-8 > wdist) do
    p.x -= 1
   end
   p.hor = 0
  end
 end
 
 if p.hor > 0 then
  if tcolhor(p.x+7+thor,p.y) then
   local wdist = p.x+7
   for i=1,thor do
    if not tcolhor(p.x+7+i,p.y) then
     wdist += 1
    end
   end
   while (p.x+7 < wdist) do
    p.x += 1
   end
   p.hor = 0
  end
 end
 
 if (p.hor < 0) p.hor += 0.5
 if (p.hor > 0) p.hor -= 0.5
 
 --jumping
 if btnp(4) and p.ver == 0 then
  p.ver = -4
  sfx(0)
 end
 
 --clamp...
 p.hor = max(p.hor,-3)
 p.hor = min(p.hor,3)
 p.ver = min(p.ver,8)
 --apply movement!
 p.x += p.hor
 p.y += p.ver
 p.x = flr(p.x)
 p.y = flr(p.y)
 p.bottom = p.y+8
 
 --for sprites
 if p.hor == 0 then
  p.idle = true
 else
  p.idle = false
 end
 
 --running animation
 if p.idle == false
 and p.ver == 0 then
  if (p.animframe == 1) sfx(3)
  if (p.animframe == 3) sfx(3)
  
  p.animframe += 0.5
  if (p.animframe == 4) p.animframe = 0
 else
  p.animframe = 0
 end
 
 --on the next level?
 if p.y < 128
 and lgain == 1 then
  level += 1
  lgain = 0
  --speed gain?
  if level >= sglvl+5 then
   scrollgain += 0.1
   sglvl = level
  end
 end
 
 --wrap
 if p.x < -8 then
  p.x += 143+abshor
 end
 if p.x > 136 then
  p.x -= 143+abshor
 end
 
 --die
 if p.y >= 264-scroll
 and death == 0 then
  pdie()
 end
 
 if btn(5)
 and death == 0 then
  pdie()
 end
end






--end of long code




--the game loop

--i plan on making a title
--screen one day
function game()
 if gstate == 0 then
  if death == 0 then
   mapupdate()
   plogic()
  else
   pghosty -= 1
   if endtimer > 0 then
    endtimer -= 1
   else
    --scoreboard
    fadeout()
    gstate = 1
    --nrecord = checkscore()
    --if nrecord != -1 then
     --scoreupdate() end
    p.x = 192
    p.y = 48
    p.dir = true
    _draw()
    fadein()
   end
   if pghostdir == false then
    pghostdir = true
   else
    pghostdir = false
   end
  end
 end
 --the secret clearscore combo
 --if gstate == 1
 --and btn(3)
 --and btnp(5) then
  --sfx(4)
  --clearscore()
 --end
 if gstate == 1 and btn(4) then
  sfx(4)
  fadeout()
  run()
 end
end

--init
function _init()
 --cartdata("clipper_01")
 game_ini()
end
--the program.
function _update()
 game()
end

function _draw()
 local rs = flr(scroll)
 cls()
 rectfill(0,0,255,255,7)
 if gstate == 0 then
 --draw map
 map(0,16,0,128,16,16)
 map(0,0,0,0,16,16)
 camera(0,128-rs)
 
 --draw ui
 rectfill(0,248-rs,127,255-rs,7)
 print("level "..level,3,248-rs,1)
 
 --drawing player
 --dead
 if death == 1 then
  pdraw(0,9,25)
  spr(11,p.x-8,pghosty,2,2,pghostdir)
 else
 --idle pose
 if p.idle == true
 and p.ver == 0 then
  --blinking
  if flr(rnd(30)) == 0 then
   pdraw(1,7,23)
  else
   pdraw(1,1,17)
  end
 end
 --running
 if p.idle == false
 and p.ver == 0 then
  local frame = flr(p.animframe)
  if (frame == 0) pdraw(0,1,3)
  if (frame == 1) pdraw(1,1,17)
  if (frame == 2) pdraw(0,1,19)
  if (frame == 3) pdraw(1,1,17)
 end
 --vertical movement
 if (p.ver < 0) pdraw(0,1,5)
 if (p.ver > 0) pdraw(0,1,21)
end
end
--draw the scoreboard
if gstate == 1 then
 camera(128,0)
 map(16,0,128,0,16,16)
 local loff = 0
 if (level >= 10) loff = 4
 if (level >= 100) loff = 8
 --draw player and expression
 local expressa = 1
 local expressb = 17
 if (level < 10) expressa = 13
 if (level > 19) expressb = 29
 pdraw(1,expressa,expressb)
 print("highest floor",166,72,1)
 print(level,190-loff,88,1)
 --draw the names
-- print("1. yi  - "..dget(0),168,28,13)
-- print("2. er  - "..dget(1),168,36,1) 
-- print("3. san - "..dget(2),168,44,1)
-- print("4. si  - "..dget(3),168,52,1)
-- print("5. wu  - "..dget(4),168,60,1)
-- print("6. liu - "..dget(5),168,68,1)
-- print("7. qi  - "..dget(6),168,76,1)
-- print("8. ba  - "..dget(7),168,84,1)
 print("press jump button",160,100,1)
 print("to try again",170,108,1)
end
end
